t1 = (1, 2, 5, 7, 9, 2, 4, 6, 8, 10)
mid = len(t1) // 2
print(t1[:mid])
print(t1[mid:])


12(b)
t1 = (1, 2, 5, 7, 9, 2, 4, 6, 8, 10)
even_tuple = tuple(x for x in t1 if x % 2 == 0)
print(even_tuple)

12(c)
t1 = (1, 2, 5, 7, 9, 2, 4, 6, 8, 10)
t2 = (11, 13, 15)
result = t1 + t2
print(result)

12(d)

t1 = (1, 2, 5, 7, 9, 2, 4, 6, 8, 10)
print("Maximum:", max(t1))
print("Minimum:", min(t1))
